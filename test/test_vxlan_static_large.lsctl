settings vxlan/static -nets {1}

proc for40 {var body} {
    for {set $var 1} {[set $var] <= 40} {incr i} $body
}

for40 i {
    phys -if out -name $i -nets {1} -ip 172.16.0.$i
}

net 1 {
    for40 i {
	 virt -if virt -phys $i -mac 00:00:00:00:00:[format %02x $i]
    }
}

claimLocal $localPhys
commit
