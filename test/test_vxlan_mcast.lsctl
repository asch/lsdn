settings vxlan/mcast -mcastIp 239.239.239.239 -nets {42 43}

phys -name a -if out -ip 172.16.0.1 -nets {42 43}
phys -name b -if out -ip 172.16.0.2 -nets {42 43}

net 42 {
    virt -if 1 -phys a
    virt -if 2 -phys a
    virt -if 1 -phys b
}

net 43 {
    virt -if 3 -phys a
    virt -if 2 -phys b
}

claimLocal $localPhys
commit
